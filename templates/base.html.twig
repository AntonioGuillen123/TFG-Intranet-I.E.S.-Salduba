<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			{% block title %}Intranet - I.E.S. Salduba
			{% endblock %}
		</title>
		<link rel="icon" href="images/favicon.ico">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"> {% block head %}{% endblock %}

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			{% block styleContent %}{% endblock %}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			{% block jsContent %}{% endblock %}
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
	</body>
</html>
