{% extends 'base.html.twig' %}

{% block head %}<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">{% endblock %}

{% block styleContent %}
	{{ encore_entry_link_tags('dashboard') }}

	{% block styleContentDashBoard %}{% endblock %}
{% endblock %}

{% block body %}
	{% include 'partials/header.html.twig' %}

	<div id="dashboard-container" class="container-fluid position-relative">
		<div class="row">
			{% include 'partials/sidebar.html.twig' %}

			<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
				<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
					<h1 class="h2">
						{% block titlePage %}
						{% endblock %}
					</h1>
					<div class="btn-toolbar mb-2 mb-md-0 gap-2 align-items-center">
						{% block toolbarPage %}
						{% endblock %}
					</div>
				</div>

				{% block pageContent %}
					<div id="main-content" class=" d-flex flex-wrap align-items-center m-4"></div>
				{% endblock %}
			</div>
		</main>


	</div>
</div>{% endblock %}
